{
  "name": "trade-service",
  "version": "1.0.0",
  "description": "Trade Serivce for Metallica",
  "repository": {
    "type": "git",
    "url": "https://amula@del.tools.publicis.sapient.com/bitbucket/scm/seaep/132650-amit-mula.git"
  },
  "author": "Amit Mula <amula@sapient.com>",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "test": "mocha",
    "lint": "esw -w src test",
    "preinstall": "npx npm-force-resolutions"
  },
  "keywords": [
    "trade-service",
    "Metallica"
  ],
  "dependencies": {
    "@amitmula/metallica-datastore-client": "^1.0.2",
    "@amitmula/metallica-pubsub-client": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "config": "^1.29.2",
    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "joi": "^13.1.0",
    "morgan": "^1.10.0",
    "pug": "^2.0.4",
    "request": "^2.74.0",
    "rimraf": "^2.6.2"
  },
  "resolutions": {
    "hoek": "^5.0.3",
    "braces": "^2.3.1",
    "cryptiles": "^4.1.2",
    "stringstream": "^0.0.6",
    "kind-of": "^6.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^6.0.1",
    "mocha": "^7.2.0",
    "nodemon": "^1.12.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "directories": {
    "test": "test"
  }
}
